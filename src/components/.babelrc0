{
  "plugins": ["react-hot-loader/babel"],
  "presets": [
    ["@babel/preset-env", {
      "modules": false
    }],
    "@babel/preset-stage-3",
    "@babel/preset-react"
  ],
  "env": {
    "production": {
      "plugins": [
        "@babel/plugin-transform-react-constant-elements",
        "@babel/plugin-transform-react-inline-elements",
        ["babel-plugin-transform-react-remove-prop-types", {
          "removeImport": true
        }]
      ]
    }
  }
}
